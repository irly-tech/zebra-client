{
    "name": "@irly-tech/zebra-client",
    "version": "0.3.4",
    "type": "module",
    "description": "Zebra API client with retry logic and telemetry support",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./testing": {
            "types": "./dist/testing/index.d.ts",
            "import": "./dist/testing/index.js",
            "require": "./dist/testing/index.cjs"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/irly-tech/zebra-client"
    },
    "scripts": {
        "build": "tsup",
        "test": "node --import tsx --test $(find src -name '*.test.ts')",
        "harness": "tsx src/bin/harness.ts"
    },
    "keywords": [
        "zebra",
        "api",
        "client",
        "savannah"
    ],
    "author": "irly-tech",
    "license": "UNLICENSED",
    "devDependencies": {
        "typescript": "^5.3.3",
        "rimraf": "^5.0.5",
        "@types/node": "^20.11.0"
    }
}