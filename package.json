{
    "name": "@irly-tech/zebra-client-monorepo",
    "private": true,
    "description": "Monorepo for irly-tech Zebra API client",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "npm run build --workspaces",
        "build:clean": "rimraf packages/*/dist && npm run build",
        "test": "npm run test --workspaces",
        "lint": "npm run lint --workspaces"
    },
    "devDependencies": {
        "rimraf": "^5.0.5",
        "tsup": "^8.0.1",
        "tsx": "^4.21.0",
        "typescript": "^5.3.3"
    }
}
